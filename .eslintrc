{
  "rules": {
    "indent": [
      2,
      4
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "valid-jsdoc": [
      2
    ],
    "no-console": [
      0
    ]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}
